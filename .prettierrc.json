{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 125,
  "endOfLine": "crlf",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "fs",
    "<THIRD_PARTY_MODULES>",
    "^[.]"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ]
}